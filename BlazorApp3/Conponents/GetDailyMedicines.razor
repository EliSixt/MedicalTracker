<h3>GetDailyMedicines</h3>

<div style="margin: 5px">
    <GetMedicine @bind-Medicine="DayToDayMedicine.Medicine" />
</div>
<div style="margin: 5px">
    <input type="text" @bind="DayToDayMedicine.DoseMg" /> @*Change dose amount*@
</div>


@code {
    [Parameter]
    public DailyMedicine DayToDayMedicine { get; set; } = new();

    [Parameter]
    public EventCallback<DailyMedicine> DayToDayMedicineChanged { get; set; }

}
